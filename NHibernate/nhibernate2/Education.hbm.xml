<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MyORMSamp"
                   namespace="MyORMSamp">
  <class name="Education">
    <id name="ID">
      <generator class="guid"/>
    </id>
    <property name="Name"/>
    <property name="Year"/>
    <many-to-one not-null="true" name="Employee" column="EmployeeID"/>
  </class>  
</hibernate-mapping>